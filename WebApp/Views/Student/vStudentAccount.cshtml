@using WebApp.Helpers;

<script src="~/Scripts/Views/Student/vStudentAccount.js"></script>
@{
    ViewBag.Title = "vStudentAccount";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    #studentAccount .modal, #editPassword.modal {
        color: #000;
    }

    .btn-orange {
        padding: 0 4px;
        white-space: normal;
        width: 140px;
    }

    #fader {
        opacity: 0.5;
        background: black;
        position: fixed;
        top: 0;
        right: 0;
        bottom: 0;
        left: 0;
        display: none;
    }

    .loader-section {
        width: 100vw;
        height: 100vh;
        max-width: 100%;
        position: fixed;
        top: 0;
        display: flex;
        justify-content: center;
        align-items: center;
        opacity: 0.5;
        background: black;
        z-index: 999;
        transition: all 1s 1s ease-out;
    }
.loaded{
  opacity:0;
  z-index:-1;
}

.loader {
    width: 48px;
    height: 48px;
    border: 5px solid #623ddb;
    border-bottom-color: transparent;
    border-radius: 50%;
    display: inline-block;
    box-sizing: border-box;
    animation: rotation 1s linear infinite;
}

    #studentAccount .nav-item:hover {
        color: #f37b24;
        text-decoration: underline;
    }

@@keyframes rotation {
    0% {
        transform: rotate(0deg);
    }
    100% {
        transform: rotate(360deg);
    }
}

    
    
</style>
<div id="loader-section" class="loader-section loaded">
    <span class="loader "></span>
</div>

<a id="btnToTop"></a>

<div id="fader">
</div>



