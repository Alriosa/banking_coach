<div>
    <div>
        @using WebApp.Helpers;
        <script src="~/Scripts/Views/Student/vStudentAccount.js"></script>
        @{
            ViewBag.Title = "vStudentAccount";
            Layout = "~/Views/Shared/_Layout.cshtml";
        }
        <a id="btnToTop"></a>

        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6">
                    <div class="container">
                        
                        <div class="row">
                            <div class="navbar navbar-expand-lg d-flex flex-column">

                                <div class="col-lg-12">
                                    <div class="collapse navbar-collapse">
                                        <ul class="navbar-nav">
                                            <li class="btn nav-item text-uppercase">Mi Currículo</li>
                                            <li class="btn nav-item text-uppercase" data-toggle="modal" data-target="#editPassword">Ajustes</li>
                                            <li class="btn nav-item text-uppercase">Enviar Currículo</li>
                                            <li class="btn nav-item text-uppercase" id="downloadCV">Descargar Currículo</li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <h2>Bienvenido <span id="P_WelcomeName"></span></h2>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-8">
                                <p>Progreso de su currículo:</p>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-success" role="progressbar" id="infoProgress" aria-valuenow="20"
                                         aria-valuemin="0" aria-valuemax="100" style="width:30%">

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row my-5">
                            <div class="col-lg-10">
                                <div>
                                    <div class="container">
                                        <div class="row">
                                            <div class="col-lg-12 my-2">
                                                <div class="title d-flex justify-content-between w-100">
                                                    <i class="fa-solid fa-hand-point-right"></i>
                                                    <p>Información personal </p>

                                                    <button class="btn btn-orange" type="button" data-toggle="modal"
                                                            data-target="#modalEditInfo">
                                                        Editar
                                                    </button>

                                                </div>
                                                <div>
                                                    <div class="modal fade" id="modalEditInfo" tabindex="-1" aria-labelledby="modalEditInfoLabel" aria-hidden="true" role="dialog">
                                                        <div class="modal-dialog  modal-lg" role="document">
                                                            <div class="modal-content">
                                                                <div class="modal-header">
                                                                    <h5 class="modal-title" id="modalEditInfoLabel">Editar información</h5>
                                                                    <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                                                                        <span aria-hidden="true">&times;</span>
                                                                    </button>
                                                                </div>
                                                                <div class="modal-body">
                                                                    <form id="frmEditInfoBasic" role="form" method="POST" action="">
                                                                        <div class="container">
                                                                            <div class="row">
                                                                                <div class="col-lg-12 my-2">
                                                                                    <div class="d-flex flex-column">
                                                                                        <input type="text" id="txtIdStudent" class="d-none" ColumnDataName="StudentID">
                                                                                        <input type="text" id="txtStudentLogin" class="d-none">
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            @Html.CtrlInputSecond(id: "txtFirstName", type: "text", required: true, placeHolder: "Primer Nombre", columnDataName: "FirstName")
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            @Html.CtrlInputSecond(id: "txtLastName", type: "text", required: true, placeHolder: "Primer Apellido", columnDataName: "FirstLastName")
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            @Html.CtrlInputSecond(id: "txtSecondLastName", type: "text", required: true, placeHolder: "Segundo Apellido", columnDataName: "SecondLastName")
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column ">
                                                                                                    <label>País</label>
                                                                                                    <select style="height:54px;" class="form-control w-100" id="txtCountry" name="Country" ColumnDataName="Country" required>
                                                                                                        <option value="CR">Costa Rica</option>
                                                                                                        <option value="NC">Nicaragua</option>
                                                                                                        <option value="VNZ">Venezuela</option>
                                                                                                    </select>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="selectedCostaRica">
                                                                                        <div class="form-row ">
                                                                                            <div class="col-lg-12">
                                                                                                @Html.CtrlDropDown(id: "txtProvince", label: "Provincia", listId: "LST_PROVINCES", columnDataName: "Province")
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-row ">
                                                                                            <div class="col-lg-12">
                                                                                                @Html.CtrlDropDown(id: "txtCanton", listId: "LST_CANTONS", label: "Cantón", columnDataName: "Canton")
                                                                                            </div>
                                                                                        </div>
                                                                                        <div class="form-row ">
                                                                                            <div class="col-lg-12">
                                                                                                @Html.CtrlDropDown(id: "txtDistrict", listId: "LST_DISTRICTS", label: "Distrito", columnDataName: "District")
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column ">
                                                                                                    <label>Tipo de documento:</label>
                                                                                                    <select style="height:54px;" class="form-control w-100" id="txtIdType" name="IdType" ColumnDataName="IdType" required>
                                                                                                        <option value="Cédula persona física">Cédula persona física</option>
                                                                                                        <option value="Residencia">Residencia</option>
                                                                                                        <option value="Pasaporte">Pasaporte</option>
                                                                                                    </select>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                @Html.CtrlInputSecond(id: "txtIdentificationNumber", type: "text", required: true, placeHolder: "Número de identificación", columnDataName: "IdentificationNumber")
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                @Html.CtrlInputSecond(id: "txtBirthdate", type: "date", required: true, placeHolder: "Fecha de Nacimiento", columnDataName: "Birthdate")

                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column">
                                                                                                    <label for="txtAge">Edad</label>
                                                                                                    <input type="number" class="form-control input-secondary" id="txtAge" placeholder="Edad" ColumnDataName="Age" disabled>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column">
                                                                                                    @Html.CtrlInputSecond(id: "txtEmail", type: "email", required: true, placeHolder: "Correo electrónico", columnDataName: "Email")

                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column">
                                                                                                    @Html.CtrlInputSecond(id: "txtPhoneNumber", type: "text", required: true, placeHolder: "Número de teléfono", columnDataName: "FirstPhoneNumber")
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div class="input-group mb-3">
                                                                                                <div class="form-group input-group flex-column">
                                                                                                    @Html.CtrlInputSecond(id: "txtSecondPhoneNumber", type: "text", required: true, placeHolder: "Número de teléfono secundario", columnDataName: "SecondPhoneNumber")
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div>
                                                                                                <div class="input-group mb-3">
                                                                                                    <div class="form-group input-group flex-column ">
                                                                                                        <label for="txtJobAvailability">Disponibilidad laboral</label>
                                                                                                        <select style="height:54px;" class="form-control w-100 " id="txtJobAvailability"
                                                                                                                name="txtJobAvailability" ColumnDataName="JobAvailability" required>
                                                                                                            <option value="Inmediata">Inmediata</option>
                                                                                                            <option value="Debo cumplir pre-aviso">Debo cumplir pre-aviso</option>
                                                                                                        </select>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div>
                                                                                                <div class="input-group mb-3">
                                                                                                    <div class="form-group input-group flex-column ">
                                                                                                        <label for="txtTransportAvailability">Disponibilidad de traslado a nivel nacional</label>
                                                                                                        <select style="height:54px;" class="form-control w-100 " id="txtTransportAvailability"
                                                                                                                name="txtTransportAvailability" ColumnDataName="TransportAvailability" required>
                                                                                                            <option value="Sí">Sí</option>
                                                                                                            <option value="No">No</option>
                                                                                                        </select>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div>
                                                                                                <div class="input-group mb-3">
                                                                                                    <div class="form-group input-group flex-column ">
                                                                                                        <label for="txtDriveLicense">Licencia de conducir</label>
                                                                                                        <div class="d-flex justify-content-around">
                                                                                                            <div>
                                                                                                                <label for="licenseA1"> <input type="checkbox" id="licenseA1" name="driverLicenses" value="A1" />A1</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseA2"> <input type="checkbox" id="licenseA2" name="driverLicenses" value="A2" />A2</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseA3">  <input type="checkbox" id="licenseA3" name="driverLicenses" value="A3" />A3</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseB1"><input type="checkbox" id="licenseB1" name="driverLicenses" value="B1" />B1</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseB2"> <input type="checkbox" id="licenseB2" name="driverLicenses" value="B2" />B2</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseB3"><input type="checkbox" id="licenseB3" name="driverLicenses" value="B3" />B3</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <label for="licenseC1"><input type="checkbox" id="licenseC1" name="driverLicenses" value="C1" />C1</label>
                                                                                                            </div>
                                                                                                            <div>
                                                                                                                <labels for="licenseC2"><input type="checkbox" id="licenseC2" name="driverLicenses" value="C2" />C2</labels>
                                                                                                            </div>
                                                                                                        </div>

                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="form-row ">
                                                                                        <div class="col-lg-12">
                                                                                            <div>
                                                                                                <div class="input-group mb-3">
                                                                                                    <div class="form-group input-group flex-column ">
                                                                                                        <label for="txtVehicle">¿Posee vehículo?</label>
                                                                                                        <select style="height:54px;" class="form-control w-100 " id="txtVehicle"
                                                                                                                name="txtVehicle" ColumnDataName="Vehicle" required>
                                                                                                            <option value="Sí">Sí</option>
                                                                                                            <option value="No">No</option>
                                                                                                        </select>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>

                                                                                    <div class="selectedVehicle">
                                                                                        <div class="form-row ">
                                                                                            <div class="col-lg-12">
                                                                                                <div class="input-group mb-3">
                                                                                                    <div class="form-group input-group flex-column ">
                                                                                                        <label for="txtTypeVehicle">¿Posee vehículo?</label>
                                                                                                        <select style="height:54px;" class="form-control w-100 " id="txtTypeVehicle"
                                                                                                                name="txtTypeVehicle" ColumnDataName="Type_Vehicle" required>
                                                                                                            <option value="Motocicleta">Motocicleta</option>
                                                                                                            <option value="Vehículo Particular">Vehículo particular</option>
                                                                                                            <option value="Camioneta">Camioneta</option>
                                                                                                            <option value="Buseta">Buseta</option>
                                                                                                            <option value="Camión">Camión</option>
                                                                                                        </select>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div>
                                                                                        </div>

                                                                                    </div>

                                                                                    <div class="row my-4 mx-auto">
                                                                                        <div style="margin: 0px auto 10px;">
                                                                                            @Html.CtrlButton(viewName: "vStudentAccount", type: "button", id: "btnSaveChangesInfo", label: "Guardar Cambios", onClickFunction: "Update", buttonType: "orange")
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </form>

                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div>
                                            <div class="row">
                                                <div class="col-lg-12 my-2">
                                                    <div class="title d-flex justify-content-between">
                                                        <i class="fa-solid fa-hand-point-right"></i>
                                                        <p>Preparación Académica</p>
                                                        <!--<button type="button" class="btn btn-orange"
                                                            id="editAcademic">
                                                        Editar
                                                    </button>-->
                                                        <a href="#academic">
                                                            <button type="button" class="btn btn-orange"
                                                                    id="goAcademic" style="width: 144px;">
                                                                Ver
                                                            </button>
                                                        </a>
                                                    </div>

                                                </div>

                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 my-2">
                                                    <div class="title d-flex justify-content-between">
                                                        <i class="fa-solid fa-hand-point-right"></i>
                                                        <p>Idiomas</p>
                                                        <a href="#languages">
                                                            <button type="button" class="btn btn-orange"
                                                                    id="goLanguages" style="width: 144px;">
                                                                Ver
                                                            </button>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12 my-2">
                                                    <div class="title d-flex justify-content-between">
                                                        <i class="fa-solid fa-hand-point-right"></i>
                                                        <p>Referencias</p>
                                                        <!--<button type="button" class="btn btn-orange"
                                                            id="editAcademic">
                                                        Editar
                                                    </button>-->
                                                        <a href="#references">
                                                            <button type="button" class="btn btn-orange"
                                                                    id="goReferences" style="width: 144px;">
                                                                Ver
                                                            </button>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6" >
                    <div class="container">
                        <div class="row">
                            <div class="box my-3">
                                <div class="box-container d-flex flex-column">
                                    <h3><strong>Mis datos personales</strong></h3>
                                    <div class="container" id="my_info">
                                        <div class="row flex-rows">
                                            <div class="col-lg-7">
                                                <p><strong>Nombre Completo: </strong><span id="P_CompleteName"></span></p>
                                                <p><strong>País Origen: </strong><span id="P_Country"></span></p>
                                                <p><span id="P_Location"></span></p>
                                                <p><strong>Tipo de documento: </strong><span id="P_Id_Type"></span></p>
                                                <p><strong>Número de Identificación: </strong><span id="P_IdentificationNumber"></span></p>
                                                <p><strong>Fecha de Nacimiento: </strong><span id="P_Age"></span></p>
                                                <p><strong>Edad: </strong><span id="P_Birthdate"></span></p>
                                                <p><strong>Correo Electrónico: </strong><span id="P_Email"></span></p>

                                            </div>
                                            <div class="col-lg-4">
                                                <p><strong>Teléfono Principal </strong><span id="P_PhoneNumber"></span></p>
                                                <p><strong>Teléfono Secundario </strong><span id="P_SecondPhoneNumber"></span></p>
                                                <p><strong>Licencia(s): </strong><span id="P_DriverLicenses"></span></p>
                                                <p><strong>¿Con Vehículo?: </strong><span id="P_Vehicle"></span></p>
                                                <p id="P_Type_Vehicle"></p>

                                                <p><strong>Disponibilidad para trabajar: </strong><span id="P_JobAvailability"></span></p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row my-5 justify-content-center">
                <div class="col-lg-11">
                    <div class="card py-4 px-3">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-10">
                                    <h3> Mi perfil profesional</h3>
                                    <hr>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-10" id="profileContentContainer">
                                    <p>Con experiencia en: <span id="profileContent"></span></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row my-5 justify-content-center">
                <div class="col-lg-11">
                    <div id="laboral" class="card py-4 px-3">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-8">
                                    <h3> Experiencia Laboral</h3>
                                </div>
                                <div class="col-lg-4" style="text-align: right;">
                                    <button class="btn" type="button" data-toggle="modal"
                                            data-target="#addLaboral">
                                        AGREGAR +
                                    </button>
                                </div>
                            </div>

                            <div class="modal fade" id="addLaboral" tabindex="-1" aria-labelledby="addLaboralLabel" aria-hidden="true" role="dialog">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title" id="addLaboralLabel">Agregar Experiencia Laboral</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div>
                                            <div class="modal-body">
                                                <form id="frmAddLaboral">
                                                    <div class="container">
                                                        <div class="row">
                                                            <div class="col-lg-12 my-2">
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtWorkPosition", type: "text", required: true, placeHolder: "Puesto", columnDataName: "WorkPosition")
                                                                </div>
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtWorkstation", type: "text", required: true, placeHolder: "Cargo", columnDataName: "Workstation")
                                                                </div>
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txCompany", type: "text", required: true, placeHolder: "Compañía", columnDataName: "Company")
                                                                </div>
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtResponsabilites", type: "text", required: true, placeHolder: "Responsabilidades", columnDataName: "Responsabilites")
                                                                </div>
                                                                <div class="form-group" lang="es-us">
                                                                    <label for="fecha" class="control-label">Fecha Ingreso</label>
                                                                    <div lang="es-us">
                                                                        <input type="month" class="form-control input-lg" name="startDate" id="txtStartDate" ColumnDataName="StartDate" min="1990-01" value="2000-08" lang="es-us">

                                                                    </div>
                                                                </div>
                                                                <div class="form-group" lang="es-us">
                                                                    <label for="fecha" class="control-label">Fecha Salida (Opcional si sigue laborando)</label>

                                                                    <div lang="es-us">
                                                                        <input type="month" class="form-control input-lg" name="endDate" id="txtEndDate" ColumnDataName="EndDate" min="1990-01" value="" lang="es-us">

                                                                    </div>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar </button>
                                            @Html.CtrlButton(viewName: "vStudentAccount", type: "button", id: "btnRegisterLaboral", label: "Registrar", onClickFunction: "ValidateInputsAddLaboral", buttonType: "orange")
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="modal fade" id="editLaboral" tabindex="-1" aria-labelledby="editLaboralLabel" aria-hidden="true" role="dialog">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title" id="editLaboralLabel">Editar Experiencia Laboral</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Cerrar">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div>
                                            <div class="modal-body">
                                                <form id="frmEditLaboral">
                                                    <div class="container">
                                                        <div class="row">
                                                            <div class="col-lg-12 my-2">
                                                                <input type="hidden" name="laboral_token" id="laboral_token" value="">
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtEditWorkPosition", type: "text", required: true, placeHolder: "Puesto", columnDataName: "WorkPosition")
                                                                </div>
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtEditWorkstation", type: "text", required: true, placeHolder: "Cargo", columnDataName: "Workstation")
                                                                </div>
                                                                <div class="form-group">
                                                                    @Html.CtrlInputSecond(id: "txtEditCompany", type: "text", required: true, placeHolder: "Compañía", columnDataName: "Company")
                                                                </div>
